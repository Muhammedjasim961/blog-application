<div class="author">
  <h1 class="author_heading">Search Results</h1>
</div>
<section class="articles">
  <ul class="article-ul">
    <% data.forEach(post => { %>

    <li>
      <a href="/post<%= post._id %>">
        <span class="post_title"><%= post.title %> </span>
        <span class="article_list_date"
          ><%= post.createAt.toDateString() %></span
        >
      </a>
    </li>

    <% }) %>
  </ul>
</section>
